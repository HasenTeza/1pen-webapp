  margin: 0 auto;
  background-color: #fff;
}

.list_nr text {
  display: block;
  padding: 10rpx 0;
  padding-left: 37rpx;
  width: 100%;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
}

.img {
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 150rpx;
}

/* 添加拼团后 */

.pintuan {
