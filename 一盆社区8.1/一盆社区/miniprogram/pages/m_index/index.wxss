/* 公告 */

.gonggao {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100rpx;
  padding-top: 20rpx;
}

.gonggao-image {
  width: 90rpx;
  height: 90rpx;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
  margin-left: 5%;
}

.gonggao-text {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 24rpx;
  color: #666;
  width: 80%;
  margin-left: 20rpx;
}

.gonggao-view {
  display: flex;
  flex-direction: row;
}

.gonggao-text-view {
  display: flex;
  flex-direction: column;
  width: 70%;
}

.gonggao-text1 {
  height: 45rpx;
  line-height: 45rpx;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.gonggao-image1 {
  width: 120rpx;
  height: 80rpx;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
  margin-left: 5%;
}

