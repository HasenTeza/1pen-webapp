/* 猜你喜欢 */

.cnxh {
  padding-top: 20rpx;
  background-color: #f1f1f1;
}

.cnxh_content {
  width: 96%;
  padding: 0 2%;
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 20rpx;
