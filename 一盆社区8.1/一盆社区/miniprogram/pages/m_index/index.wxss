/* //搜索 */

.veiwBox {
  position: relative;
}

.share {
  padding: 0;
  margin: 0;
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  z-index: 100;
  text-indent: -9999rpx;
  width: 80rpx;
  height: 80rpx;
  background-image: url(http://www.1psq.cn/xcx_img/14.png);
  background-repeat: no-repeat;
  background-position: -3rpx -3rpx;
  background-size: 110% 110%;
  border-radius: 50%;
  overflow: hidden;
}

.searchBox {
  width: 25%;
  padding: 5rpx 0;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 35rpx;
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  z-index: 1;
}

.search {
  width: 94%;
  height: 60rpx;
  /* background-color: #fff; *//* border: 1rpx solid #fc6f68; */
  margin: 0 auto;
  position: relative;
}

.searchcion {
  position: absolute;
  left: 6%;
  top: 17rpx;
}

.searchtext {
  display: block;
  margin-right: 30rpx;
  font-size: 24rpx;
  line-height: 56rpx;
  text-align: right;
  color: #fff;
}

/* //主体样式 */

.content {
  width: 100%;
}

.banner {
  width: 100%;
}

swiper {
  height: 400rpx;
}

.first_swiper-item {
  width: 100%;
  height: 100%;
}

/* 导航 */

.nav {
  width: 100%;
  padding: 20rpx 0;
  /* height: 190rpx; *//* background-color:forestgreen; */
}

.nav_list {
  width: 25%;
  height: 100%;
  float: left;
}

.nav_list image {
  display: block;
  width: 90rpx;
  height: 90rpx;
  margin: 13rpx auto 7rpx;
}

.nav_list text {
  display: block;
  text-align: center;
  font-size: 24rpx;
  line-height: 72rpx;
}

/* 主题内容 */

.neirong {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  background-color: #f4f3f8;
}

/* 缴费 */

.jiaofei {
  width: 88%;
  height: 400rpx;
  margin: 40rpx 6%;
  border-radius: 20rpx;
  background-image: url(http://www.1psq.cn/xcx_img/3.png);
  background-position: -120rpx -20rpx;
  background-size: 140% 120%;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
}

.neirong  .jiaofei1 view:nth-child(1) {
  padding-top: 150rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  text-align: center;
}

.neirong  .jiaofei1 view:nth-child(2) {
  width: 200rpx;
  height: 80rpx;
  margin: 0 auto;
  text-align: center;
  border: 3rpx solid #ccc;
  border-radius: 10rpx;
  font-size: 32rpx;
  font-weight: none;
  line-height: 80rpx;
}

.jiaofei view:nth-child(1) {
  height: 80rpx;
  line-height: 80rpx;
  padding-top: 46rpx;
}

.jiaofei view:nth-child(2) {
  height: 140rpx;
  font-size: 60rpx;
  font-weight: bold;
  line-height: 140rpx;
}

.jiaofei view:nth-child(3) {
  width: 100%;
  height: 74rpx;
}

.jiaofei view:nth-child(3) text {
  float: left;
  margin-left: 16%;
  width: 25%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  border: 2rpx solid #f8f5cc;
  border-radius: 20rpx;
}

/* 当前和历史缴费 */

.lists {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
}

/* 单个list */

.list {
  width: 88%;
  height: 298rpx;
