.add_bj {
  width: 100%;
  height: 100%;
  background-color: #f5f4f9;
  position: relative;
}

.xian {
  width: 100%;
  height: 5rpx;
}

.xian image {
  display: block;
  width: 100%;
  height: 100%;
}

.user_xx {
  width: 100%;
  height: 150rpx;
  background-color: #fff;
}

.left {
  float: left;
  width: 150rpx;
  height: 150rpx;
}

.left image {
  width: 50rpx;
  height: 50rpx;
  padding: 50rpx;
}

.content {
  float: left;
  width: 55%;
  padding-top: 20rpx;
  height: 130rpx;
  overflow: hidden;
}

.content text {
  display: block;
  font-size: 32rpx;
  line-height: 56rpx;
}

.right {
  float: right;
  width: 150rpx;
  height: 150rpx;
}

.right image {
  width: 50rpx;
  height: 50rpx;
  padding: 50rpx;
}

.list {
  margin: 3%;
  margin-bottom: 0;
  width: 94%;
  height: 120rpx;
  background-color: #fff;
}

.list_icon1 {
  float: left;
  width: 120rpx;
  height: 120rpx;
}

.list_icon1 image {
  width: 40rpx;
  height: 40rpx;
  padding: 40rpx;
}

.list_text {
  float: left;
  font-size: 32rpx;
  line-height: 120rpx;
}

.list_icon2 {
  float: right;
  width: 120rpx;
  height: 120rpx;
}

.list_icon2 image {
  width: 40rpx;
  height: 40rpx;
  padding: 40rpx;
}

.list_n {
  float: right;
  font-size: 24rpx;
  line-height: 120rpx;
  color: #ff6b6b;
}

.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background-color: #fff;
}

.footer_left {
  float: left;
  color: #ff6b6b;
  font-size: 48rpx;
  line-height: 60rpx;
  width: 50%;
  padding-left: 30rpx;
}
.footer_left text{
  display: block;
}
.footer_left text:nth-child(2){
  font-size: 24rpx;
  line-height: 40rpx;
  color: #888;
  text-indent: 10rpx;
}
.footer_right{
  float: right;
  width: 200rpx;
  height: 100%;
  background-color: #ff6b6b;
  color: #fff;
  font-size: 32rpx;
  line-height: 100rpx;
  text-align: center;
}

/* //弹层 */
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  height: 500rpx;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
}




/* 服务类型弹框样式 */
.t_header{
  width: 100%;
  height: 100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  text-align: center;
  position: absolute;
  left: 0;
  top: 0;
}
.t_cha{
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  width: 40rpx;
  height: 40rpx;
  /* text-indent: -9999rpx; */

}
.t_cha image{
  width: 100%;
  height: 100%;
}
.t_content{
  padding: 5%;
  margin-top: 100rpx;
  width: 90%;
}
.t_list{
  width: 100%;
  height: 100rpx;
}
.t_list image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  padding: 30rpx;
}
.t_text{
  float: left;
  font-size: 32rpx;
  line-height: 100rpx;
}

.t_ms{
  float: left;
  padding-left: 20rpx;
  font-size: 24rpx;
  line-height: 100rpx;
  color: #888;
}
.t_view{
  float: right;
  width: 170rpx;
  height: 100rpx;
  font-size: 30rpx;
  line-height: 100rpx;
  color: #888;
}
.t_view text{
  box-sizing: border-box;
  border: 1rpx solid #000;
  width: 60rpx;
  height: 40rpx;
  text-align: right;
  line-height: 35rpx;
  padding: 0 10rpx;
  margin: 0 5rpx;
  color: #000;
}
.t_view .xing{
  width:24rpx;
  height:24rpx;
  padding: 0;
}
.section{
   background:#CABBC7;
   margin:20rpx;
   padding:20rpx

}
/* //订单框 */
.dd_content{
  padding-top: 81rpx;
  width: 90%;
  margin: 0 5%;
  position: relative;
}
.dd_list1{
  width: 100%;
  height: 100rpx;
  margin-bottom: 4rpx;
  background-color: #fff;
}
.dd_h{
  width: 100%;
  height: 100rpx;
}
.dd_h image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  padding: 30rpx;
}
.dd_n{
  float: left;
  padding: 15rpx 10rpx;
}
.dd_n text{
  display: block;
  width: 100%;
  font-size: 28rpx;
  line-height: 36rpx;
}
.dd_list .dd_n text{
  line-height: 70rpx;
} 
.dd_list2{
  font-size: 28rpx;
  line-height: 100rpx;
}
.dd_list2 text:nth-child(1){
  float: left;
  padding: 0 40rpx;
}

.dd_list2 text:nth-child(2){
  float: right;
  padding: 0 20rpx;
}
.dd_list22 text:nth-child(3){
  float: right;
  padding: 0 20rpx;
}
.dd_list22 image{
  float: right;
  width: 40rpx;
  height: 40rpx;
  padding: 30rpx;
}
.dd_list3 image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  padding: 30rpx;
}
.dd_list3 text{
  float: left;
  font-size: 28rpx;
  line-height: 100rpx;

}
.dd_list3 input{
  float: left;
  padding-left: 20rpx;
  width: 70%;
  height: 100rpx;
  font-size: 28rpx;
  line-height: 100rpx;
}
.dd_footer{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
}
.dd_footer text{
  float: left;
  font-size: 28rpx;
  line-height: 100rpx;
  padding: 0 10rpx;
}
.dd_footer text:nth-child(2){
  padding-left: 130rpx;
}
.dd_footer view{
  float: right;
  width: 200rpx;
  height: 100rpx;
  background: #ff6b6b;
  color: #fff;
  font-size: 38rpx;
  line-height: 100rpx;
  text-align: center;
}